{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/favicon.(ico|png)",
      "status": 204,
      "continue": false
    },
    {
      "src": "/api/(.*)",
      "dest": "dist/main.js"
    },
    {
      "src": "/health",
      "dest": "dist/main.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "dist/main.js"
    },
    {
      "src": "/chat/(.*)",
      "dest": "dist/main.js"
    },
    {
      "src": "/messaging/(.*)",
      "dest": "dist/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "status": 404
    }
  ]
}